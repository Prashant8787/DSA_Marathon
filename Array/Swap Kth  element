#include<iostream>

using namespace std;

int main()

{

    

    int n,k; cin>>n>>k; 

    int a[n]; 

    for(int i=0;i<n;i++){ 

    cin>>a[i];

    }   

    int b,c;

     b=a[k-1]; 

    c=a[n-k]; 

     a[k-1]=c; 

    a[n-k]=b;

    for(int i=0;i<n;i++){ 

    cout<<a[i]<<" ";

    

    }  

    

    return 0;

}
